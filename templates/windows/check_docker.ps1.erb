$service = Get-Service "docker" -ErrorAction Ignore
if ($service -ne $null) {
    <% if @ensure == 'absent' -%>
    exit 1
    <% else %>
    exit 0
    <% end -%>
} 

if (Test-Path -Path "$env:ProgramFiles\docker") {
    <% if @ensure == 'absent' -%>
    exit 1
    <% else %>
    exit 0
    <% end -%>
}

<% if @ensure == 'absent' -%>
exit 0
<% else %>
exit 1
<% end -%>